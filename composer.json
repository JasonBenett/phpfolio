{
    "name": "what-the-fabrik/phpfolio",
    "type": "library",
    "keywords": ["PHPfolio", "What The Fabrik", "WTF", "php", "portfolio"],
    "description": "Phpfolio is a library facilitating the creation of a portfolio.",
    "license": "MIT",
    "authors": [{
            "name": "Jason Benedetti",
            "email": "jason.benedetti@live.fr"
        },
        {
            "name": "What The Fabrik Community",
            "homepage": "https://whatthefabrik.fr"
        }
    ],
    "require": {
        "php": " ^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "JasonBenett\\Phpfolio\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.83",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
